  {
    "manifest_version": 2,
	"name": "金洋投注",
    "version": "1.0.0.1",
    "description": "金洋娱乐11选5自动投注",
    "icons": {
      "16": "image/png16.png",
      "32": "image/png32.png",
      "48": "image/png48.png",
      "128": "image/png128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background_script.js"]
    },
    "permissions": ["http://jya.xinw188.com/*", 
					"activeTab",
					"cookies",
					"storage",
					"tabs",
					"http://*.lizudi.top/*"
					],
    "browser_action": {
      "default_icon": "image/png16.png",
      "default_popup": "popup.html"
    },
	"content_scripts":[
	{
		"matches":["http://jya.xinw188.com/*"],
		"js":["js/jquery211.min.js","js/tools.js","contentScript.js","contentScript_beeting.js","contentScript_post_bet.js","contentScript_test.js"],
		"run_at":"document_idle"
	}
	]
  }